{
    "builds": [{
        "src": "App/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": {
            "maxLambdaSize": "20mb"
        }
    }],
    "routes": [{
            "src": "/(.*)",
            "dest": "App/wsgi.py"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/error.html"
        }
    ]
}